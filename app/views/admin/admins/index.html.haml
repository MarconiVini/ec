- entity = Admin
%table.table.table-hover
  %tr
    %th
      =entity.human_attribute_name(:name)
    %th
      =entity.human_attribute_name(:email)    
    %th
      =entity.human_attribute_name(:created_at)
    %th
      Editar  
    %th
      Deletar
  - @admins.each do |admin|
    %tr
      %td
        =admin.name
      %td
        =admin.email
      %td
        =admin.created_at
      %td
        edit_link
      %td
        delete_link

=will_paginate @admins


